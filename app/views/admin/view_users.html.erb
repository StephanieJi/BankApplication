<div class="center jumbotron">
<h2>Users</h2>
<br>
  <h4>
	<% @users.each do |user| %>
		<div>
			<%= user.name %>
			<%= user.email %>
			<%= link_to "view transaction history", view_transaction_history_path(user) %>
			<%= link_to "delete this user", delete_user_path(user) %>
		</div>
	<% end %>
  </h4>
<br>
  <%= link_to "Log out", admin_index_path() %>
</div>


